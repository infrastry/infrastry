{
  "name": "infrastry",
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/infrastry/infrastry.git"
  },
  "author": {
    "name": "Il Harper",
    "email": "afanyiyu@hotmail.com",
    "url": "https://ilharper.com"
  },
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "yarn build:js && yarn build:css",
    "build:js": "tsc -b tsconfig.json",
    "build:css": "sass --style=compressed src/styles/index.scss lib/styles/index.css",
    "watch": "concurrently \"yarn:watch:js\" \"yarn:watch:css\"",
    "watch:js": "tsc -b -w tsconfig.json",
    "watch:css": "sass -w src/styles/index.scss lib/styles/index.css"
  },
  "devDependencies": {
    "@types/use-persisted-state": "^0",
    "normalize.css": "^8"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@use-it/event-listener": "^0",
    "use-persisted-state": "^0"
  }
}
